generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model anagrafica {
  ID Int @id @default(autoincrement())
  firstName String
  lastName String
  eta Int
  cell String
}

